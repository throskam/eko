{
  "name": "@throskam/eko",
  "version": "0.3.1",
  "description": "Multiple projects management tool",
  "repository": "throskam/eko",
  "homepage": "https://github.com/throskam/eko",
  "bugs": "https://github.com/throskam/eko/issues",
  "author": "Thibaut Roskam <throskam@gmail.com>",
  "license": "MIT",
  "keywords": [
    "multiple",
    "projects",
    "management",
    "tool",
    "cli",
    "repositories",
    "git",
    "microservices"
  ],
  "bin": {
    "eko": "src/index.js"
  },
  "main": "index.js",
  "scripts": {
    "build:linux": "pkg . --target node10-linux-x64 --output bin/eko_linux_x64",
    "build:macos": "pkg . --target node10-mac-x64 --output bin/eko_macos_x64",
    "build:windows": "pkg . --target node10-windows-x64 --output bin/eko_windows_x64",
    "release": "standard-version"
  },
  "pkg": {
    "scripts": "src/**/*.js"
  },
  "dependencies": {
    "boxen": "^3.2.0",
    "chalk": "^2.4.2",
    "glob": "^7.1.4",
    "inquirer": "^6.3.1",
    "ora": "^3.4.0",
    "pretty-time": "^1.1.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "pkg": "^4.4.0",
    "standard-version": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
